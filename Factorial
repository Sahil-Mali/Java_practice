import java.lang.*;
import java.util.Scanner;

class Practice {
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n,i,fact=1;
        System.out.println("Enter the number: ");
        n=sc.nextInt();

        for(i=n;i>=1;i--){
            fact= fact*i;
        }
        System.out.println("The factorial of "+n+" is "+fact);
    }
}
